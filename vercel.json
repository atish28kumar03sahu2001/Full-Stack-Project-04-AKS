{
    "builds":[
      {
          "src": "index.js",
          "use": "@vercel/node"
      },
      {
          "src": "dist/**",
          "use": "@vercel/static"
      }
  ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "dist/index.html"
      },
      {
        "src": "/signup",
        "dest": "dist/index.html"
      },
      {
        "src": "/signin",
        "dest": "dist/index.html"
      },
      {
          "src": "/user",
          "dest": "dist/index.html"
      },
      {
        "src": "/user/(.*)",
        "dest": "dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "dist/$1"
    }
    ]
  }